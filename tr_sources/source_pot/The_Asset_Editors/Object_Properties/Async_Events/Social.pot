#: GMS2-Robohelp%5CThe_Asset_Editors%5CObject_Properties%5CAsync_Events%5CSocial.htm%2Bhtml.body.p:160-3
msgid "Below you can find a small snippet of code as an example of use:"
msgstr ""

#: GMS2-Robohelp%5CThe_Asset_Editors%5CObject_Properties%5CAsync_Events%5CSocial.htm%2Bhtml.body.p:161-3
msgid ""
"{ANY_CODE} var ident = ds_map_find_value(async_load, \"id\" );<br /> if "
"ident == achievement_challenge_completed;<br /> {nbsp_x1} {nbsp_x1}{<br /> "
"{nbsp_x1} {nbsp_x1}player_id = ds_map_find_value(async_load, \"playerid\");"
"<br /> {nbsp_x1} {nbsp_x1}issuer_id = ds_map_find_value(async_load, "
"\"issuerid\");<br /> {nbsp_x1}state = ds_map_find_value(async_load, \"state\""
");<br /> {nbsp_x1} {nbsp_x1}date_completed = ds_map_find_value(async_load, "
"\"completeddate\");<br /> {nbsp_x1} {nbsp_x1}date_issued = ds_map_find_value("
"async_load, \"issueddate\");<br /> {nbsp_x1} {nbsp_x1}ach_type = "
"ds_map_find_value(async_load, \"type\");<br /> {nbsp_x1} {nbsp_x1}ach_ident "
"= ds_map_find_value(async_load, \"identifier\");<br /> {nbsp_x1} {nbsp_x1}"
"ach_score = ds_map_find_value(async_load, \"score\");<br /> {nbsp_x1} "
"{nbsp_x1}}"
msgstr ""

