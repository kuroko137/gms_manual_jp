#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CData_Structures%5CDS_Maps%5Cds_map_add_map.htm%2Bhtml.body.p:117-3
msgid ""
"{ANY_CODE} var j_map = ds_map_create();<br /> var j_list = ds_list_create();"
"<br /> var sub_map = ds_map_create();<br /> ds_map_add_list(sub_map, \"list\""
", j_list);<br /> ds_map_add(sub_map, \"array\", j_array);<br /> "
"ds_map_add_map(j_map, \"map\", sub_map);<br /> var j = json_encode(j_map);"
"<br /> ds_map_destroy(j_map);"
