msgid ""
"{ANY_CODE} var p_num;<br /> p_num = parameter_count();<br /> if p_num &gt; "
"0<br /> {nbsp_x3}{ <br /> {nbsp_x3}var i;<br /> {nbsp_x3}for (i = 0; i &lt; "
"p_num; i += 1)<br /> {nbsp_x6}{ <br /> {nbsp_x6}p_string[i] = "
"parameter_string(i + 1);<br /> {nbsp_x6}} <br /> {nbsp_x3}}"
