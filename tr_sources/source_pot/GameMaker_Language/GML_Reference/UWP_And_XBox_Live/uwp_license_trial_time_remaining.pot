#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CUWP_And_XBox_Live%5Cuwp_license_trial_time_remaining.htm%2Bhtml.body.p:96-3
msgid ""
"{ANY_CODE} if uwp_license_trial_version()<br /> {nbsp_x4}{ <br /> {nbsp_x4}"
"var secs = uwp_license_trial_time_remaining() mod 60;<br /> {nbsp_x4}var "
"mins = uwp_license_trial_time_remaining() div 60;<br /> {nbsp_x4}var hours = "
"mins div 60;<br /> {nbsp_x4}if secs &lt; 10 secs = \"0\" + string(secs) else "
"secs = string(secs);<br /> {nbsp_x4}if mins &gt; 60 mins -= (hours * 60);<br "
"/> {nbsp_x4}if mins &lt; 10 mins = \"0\" + string(mins) else mins = "
"string(mins);<br /> {nbsp_x4}if hours &gt; 9 hours = \"9\" else hours = "
"string(hours);<br /> {nbsp_x4}time_string = hours + \":\" + mins + \":\" + "
"secs;<br /> {nbsp_x4}}"
msgstr ""

