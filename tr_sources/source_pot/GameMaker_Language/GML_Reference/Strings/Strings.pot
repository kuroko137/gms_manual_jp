msgid "or"
msgstr ""

#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CStrings%5CStrings.htm%2Bhtml.body.p:102-3
msgid "{ANY_CODE} \"\\u00e2\" + \"a\""
msgstr ""

#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CStrings%5CStrings.htm%2Bhtml.body.p:103-3
msgid ""
"as just using \"<tt>\\u00e2a</tt>\" would actually result in the Unicode "
"character \"<tt>ส</tt>\" (essentially becoming \"<tt>\\ue2a</tt>\")."
msgstr ""

#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CStrings%5CStrings.htm%2Bhtml.body.p:104-3
