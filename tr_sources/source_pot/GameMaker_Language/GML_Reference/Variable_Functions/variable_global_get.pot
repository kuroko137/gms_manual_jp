msgid ""
"{ANY_CODE} if variable_global_exists(\"enemy_num\")<br /> {nbsp_x3}{ <br /> "
"{nbsp_x3}show_debug_message(\"enemy_num = \" + string(variable_global_get("
"\"enemy_num\"));)<br /> {nbsp_x3}}"
msgstr ""

#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CVariable_Functions%5Cvariable_global_get.htm%2Bhtml.body.p:114-3
