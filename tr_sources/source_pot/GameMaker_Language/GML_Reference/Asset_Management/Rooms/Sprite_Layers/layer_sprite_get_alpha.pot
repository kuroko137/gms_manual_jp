msgid ""
"{ANY_CODE} var lay_id = layer_get_id(\"sprite_sky\");<br /> var spr_id = "
"layer_sprite_get_id(lay_id, \"Clouds\");<br /> if "
"layer_sprite_get_alpha(spr_id) &lt; 0.1<br /> {nbsp_x4}{ <br /> "
"{nbsp_x4}layer_sprite_destroy(spr_id); <br /> {nbsp_x4}}"
