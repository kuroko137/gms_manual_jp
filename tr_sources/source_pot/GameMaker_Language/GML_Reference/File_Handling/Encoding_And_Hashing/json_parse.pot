#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CFile_Handling%5CEncoding_And_Hashing%5Cjson_parse.htm%2Bhtml.body.p:110-3
msgid ""
"In this example we will parse the following JSON string which we'll hold in "
"the variable \"json\":"
msgstr ""

#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CFile_Handling%5CEncoding_And_Hashing%5Cjson_parse.htm%2Bhtml.body.p:111-3
msgid ""
"{ANY_CODE} json = \"{\\\"myObj\\\": { \\\"apples\\\":10, \\\"oranges\\\":12, "
"\\\"potatoes\\\":100000, \\\"avocados\\\":0 }, \\\"myArray\\\":[0, 1, 2, 2, "
"4, 0, 1, 5, 1]}\""
msgstr ""

#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CFile_Handling%5CEncoding_And_Hashing%5Cjson_parse.htm%2Bhtml.body.p:112-3
msgid "This string would be parsed something like this:"
msgstr ""

#: GMS2-Robohelp%5CGameMaker_Language%5CGML_Reference%5CFile_Handling%5CEncoding_And_Hashing%5Cjson_parse.htm%2Bhtml.body.p:113-3
msgid ""
"{ANY_CODE} data = json_parse(json);<br /> if variable_struct_exists(data, "
"\"myObj\")<br /> { <br /> if is_struct(data.myObj)<br /> {nbsp_x2} {nbsp_x1}{"
"<br /> {nbsp_x2} {nbsp_x1}var _names = variable_struct_get_names(data.myObj);"
"<br /> {nbsp_x2} {nbsp_x1}var _str = \"\";<br /> {nbsp_x2} {nbsp_x1}for (var "
"i = 0; i &lt; array_length(_names); i++;)<br /> {nbsp_x2} {nbsp_x3} "
"{nbsp_x1}{<br /> {nbsp_x2} {nbsp_x3} {nbsp_x1}_str = _names[i] + \": \" "
"+{nbsp_x1}string(variable_struct_get(data.myObj, _names[i]));<br /> {nbsp_x2}"
" {nbsp_x3} {nbsp_x1}show_debug_message(_str);<br /> {nbsp_x2} {nbsp_x3} "
"{nbsp_x1}}<br /> {nbsp_x2} {nbsp_x1}}<br /> if variable_struct_exists(data, "
"\"myArray\")<br /> {nbsp_x2} {nbsp_x1}{<br /> {nbsp_x2} {nbsp_x1}if "
"is_array(data.myArray)<br /> {nbsp_x2} {nbsp_x3} {nbsp_x1}{<br /> {nbsp_x2} "
"{nbsp_x3} {nbsp_x1}show_debug_message(data.myArray);<br /> {nbsp_x2} "
"{nbsp_x3} {nbsp_x1}}<br /> {nbsp_x2} {nbsp_x1}}<br /> }"
msgstr ""

